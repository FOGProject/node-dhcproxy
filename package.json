{
  "name": "cuckoo",
  "version": "0.0.6",
  "description": "cuckoo: when you need a flying PXE server",
  "author": "Piero Proietti @pieroproietti",
  "bin": {
    "cuckoo": "./bin/run"
  },
  "homepage": "https://penguins-eggs.net",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/pieroproietti/penguins-cuckoo",
  "files": [
    "/bin",
    "/dist",
    "/pnpm-lock.yaml",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.16.4",
    "@oclif/plugin-help": "^5.1.14",
    "netmask": "^2.0.2",
    "node-static": "^0.7.11",
    "tftp": "^0.1.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@oclif/plugin-autocomplete": "1.3.1",
    "@oclif/plugin-help": "^5.1.14",
    "@oclif/test": "^2.2.1",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/netmask": "^1.0.30",
    "@types/node": "^18.7.23",
    "@types/node-static": "^0.7.7",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9.2.2",
    "mustache": "^4.2.0",
    "penguins-oclif": "^9.1.37",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.3"
  },
  "oclif": {
    "bin": "cuckoo",
    "dirname": "cuckoo",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "topics": {
      "cuckoo": {
        "description": "when you need a flying PXE server"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -p .",
    "deb": "oclif pack:deb",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/pieroproietti/penguins-cuckoo/issues",
  "keywords": [
    "pxe",
    "proxy-dhcp",
    "dhcp",
    "tfpt",
    "http"
  ],
  "types": "dist/index.d.ts"
}